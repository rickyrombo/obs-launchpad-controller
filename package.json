    "@fortawesome/fontawesome-free": "^5.12.0",
    "@reduxjs/toolkit": "^1.0.4",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "electron": "8.2.4",
    "electron-builder": "^22.4.1",
    "exports-loader": "^0.7.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "localforage": "^1.7.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
